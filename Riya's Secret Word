#include <iostream>
#include <string>
#include <algorithm>
using namespace std;

int main(){
    string word; 
    cout << "Enter word: ";
    cin >> word; 
    
    string reversed = string(word.rbegin(), word.rend()); 
    
    if(word == reversed) 
        cout << "Perfect Secret Word"; 
    else 
        cout << "Not a Secret Word"; 
    return 0; 
}
