#include <iostream>
using namespace std;

int main() {
    int A[10][7]; 
    int totalPresent[10] = {0}; 
    int totalDay[7] = {0}; 
    
    cout << "Enter attendance (10 students, 7 days):\n"; 
    for (int i = 0; i < 10; i++) { 
        for (int j = 0; j < 7; j++) { 
            cin >> A[i][j]; 
            totalPresent[i] += A[i][j]; 
            totalDay[j] += A[i][j]; 
        } 
    } 
    
    cout << "\nTotal Present Days per Student:" << endl; 
    for (int i = 0; i < 10; i++) { 
        cout << "Student " << i + 1 << ": " << totalPresent[i] << endl; 
    } 
    // Logic for highest attendance student and lowest day...
    return 0; 
}
